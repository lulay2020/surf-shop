<nav aria-label="Page navigation example">
	<ul class="pagination">
	<% if(posts.page - 1) {%>
	<li class="page-item">
		<a href="<%= paginateUrl %><%= posts.page - 1 %>" class="page-link" aria-label="Previous">
			<span aria-hidden="true">&laquo;</span>
        	<span class="sr-only">Previous</span>
		</a>
	</li>
	<% } %>
	<% if(posts.pages > 1 && posts.docs.length) { %>
	<% for(let i = start; i < end; i++){ %>
		<li class="page-item <%= i === posts.page ? 'active' : ''%>">
			<a 	href="<%= paginateUrl %><%= i %>" 
				class="page-link"
			><%= i %>
			</a>
		</li>
	<% } %>
	<% } %>
	<% if((posts.page + 1) <= posts.pages) {%>
		<a href="<%= paginateUrl %><%= posts.page + 1 %>" class="page-link" aria-label="Next">
			<span aria-hidden="true">&raquo;</span>
        	<span class="sr-only">Next</span>
		</a>
	<% } %>	
	</ul>
</nav>